  },
  "homepage": "https://github.com/lgmarch/1812645-typoteka-3#readme",
  "devDependencies": {
    "eslint": "8.5.0",
    "eslint-config-htmlacademy": "0.6.0",
    "jest": "28.1.3",
    "nodemon": "^2.0.19",
    "pino-pretty": "^4.2.1",
    "sqlite3": "5.0.11",
    "supertest": "6.2.4"
  },
  "dependencies": {
    "axios": "0.27.2",
    "cross-env": "7.0.3",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "joi": "17.6.0",
    "lodash": "^4.17.21",
    "multer": "1.4.5-lts.1",
    "nanoid": "^3.3.4",
    "pg": "8.7.3",
    "pino": "6.6.0",
    "pug": "3.0.2",
    "sequelize": "6.21.3",
    "snack-cli": "^2.0.0"
  }
}
